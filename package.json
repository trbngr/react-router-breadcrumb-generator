  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rimraf lib/",
    "build": "babel src/ --out-dir lib",
    "test": "mocha --require babel-register",
    "prepublish": "npm version patch && git push"
  },
  "keywords": [
    "react-router",
    "breadcrumbs"
  ],
  "author": "Christopher Martin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:trbngr/react-router-breadcrumb-generator.git"
  },
  "homepage": "https://github.com/trbngr/react-router-breadcrumb-generator",
  "dependencies": {
    "invariant": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "rimraf": "^2.5.0"
  }
}
